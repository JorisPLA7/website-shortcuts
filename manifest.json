{

  "manifest_version": 2,
  "name": "SearchbarShortcut",
  "version": "1.0",

  "description": "Press Ctrl + Space to open your page's search bar.",

  "icons": {
    "48": "icons/magnifier.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "run_at": "document_end",
      "js": ["payload.js"]
    }
  ]

}
